    "version": "0.2.0",
    "configurations": [
        {
            "type": "ERDB",
            "request": "launch",
            "name": "ERDB: Launch",
            "program": "${workspaceRoot}/target/debug/<binary-name>",
            "chip": "<chip-name>",
            "cwd": "${workspaceRoot}",
            "port": 8800
        },
        {
            "type": "ERDB",
            "request": "attach",
            "name": "ERDB: Attach",
            "program": "${workspaceRoot}/target/debug/<binary-name>",
            "chip": "<chip-name>",
            "cwd": "${workspaceRoot}",
            "port": 8800,
            "reset": false
        }
    ]
}